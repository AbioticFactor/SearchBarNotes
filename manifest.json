{
  "name": "SearchBarNotes",
  "version": "1.0",
  "description": "Take notes with the google searchbar!",
  "permissions": ["activeTab"],
  "omnibox": { "keyword" : "n" },
  "icons": {
    "16": "images/icon16.png"
  },
  "background": {
    "scripts": ["popup.js", "background.js"],

    "persistent": false
  },


  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {


      "128": "images/icon128.png"
    }
  },

  "manifest_version": 2
}
